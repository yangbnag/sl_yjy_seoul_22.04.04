
n1, n2 = map(int, input(). split())
m1, m2 = map(int, input(). split())
k1, k2 = map(int, input(). split())

print(f'Case #1: {n1} + {n2} = {n1+n2}')
print(f'Case #1: {m1} + {m2} = {m1+m2}')
print(f'Case #1: {k1} + {k2} = {k1+k2}')

